                           <collation>
										
                              
                              
                              
                              <p>Quires of mostly 8 leaves each. 2 inner quires are parchment, the rest is paper.</p>
									
                           
                           
                           
                           </collation>
									
                           
                           
                           
                           <condition>
										
                              
                              
                              
                              <p>Stained.</p>
										
                              
                              
                              
                              <p>Water-damaged.</p>
										
                              
                              
                              
                              <p>First leaf injured.</p>
									
                           
                           
                           
                           </condition>
								
                        
                        
                        
                        </supportDesc>
								
                        
                        
                        
                        <layoutDesc>
									
                           
                           
                           
                           <layout columns="1" ruledLines="24" writtenLines="24">
										
                              
                              
                              
                              <p>Uniform layout.</p>
										
                              
                              
                              
                              <p>Instead of catchwords, catch-letters are used, the first letter of the first word of a folio being written at the end of the preceding page.</p>
										
                              
                              
                              
                              <p>Many corrections and variant readings have been supplied in the margin.</p>
									
                           
                           
                           
                           </layout>
								
                        
                        
                        
                        </layoutDesc>
							
                     
                     
                     
